# 服务器监听的端口号
PORT=3000
# 运行环境：production（生产环境）/ development（开发环境）
NODE_ENV=production
# PostgreSQL 数据库连接地址（格式：postgresql://用户名:密码@主机地址:端口/数据库名）
DATABASE_URL=postgresql://root:your_server_passord@datbase_IP:database_port/pipeline_manager
# 数据库是否启用 SSL 加密连接
DB_SSL=false
# 数据库连接池的最大连接数
DB_POOL_MAX=10
# JWT（JSON Web Token）签名密钥，用于用户身份认证
JWT_SECRET=huawei_pipeline_secret_key_2026
# 允许跨域请求的来源地址（CORS 白名单）
CORS_ORIGIN=http://localhost:5000
# 日志输出级别（info / debug / warn / error）
LOG_LEVEL=info








